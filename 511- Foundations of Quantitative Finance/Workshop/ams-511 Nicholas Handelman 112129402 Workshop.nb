  RowBox[{
   RowBox[{
    RowBox[{
     SubscriptBox["r", "i"], "(", "t", ")"}], "-", 
    SubscriptBox["r", "f"]}], "=", 
   RowBox[{
    RowBox[{"\[Beta]", "(", 
     RowBox[{
      RowBox[{
       SubscriptBox["r", "M"], "(", "t", ")"}], "-", 
      SubscriptBox["r", "f"]}], ")"}], "+", 
    RowBox[{
