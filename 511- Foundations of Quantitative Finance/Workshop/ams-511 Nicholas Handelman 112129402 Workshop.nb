Cell[TextData[{
 "Perform a CAPM  linear regression to estimate the betas and error variances \
of each member of the DJI. For simplicity assume an annual risk free rate of \
2%. Approximate the ",
 StyleBox["daily",
  FontSlant->"Italic"],
 " rate as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", "0.02"}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"1", "/", "250"}], ")"}]}], "-", "1"}], TraditionalForm]],
  ExpressionUUID->"79c6450d-d88c-4f85-a1af-eff23ccca92e"],
 "."
}], "ItemNumbered",
 CellChangeTimes->{{3.752091302593513*^9, 3.752091440633685*^9}, {
  3.752092230123786*^9, 3.7520923903273153`*^9}, {3.7521676595030327`*^9, 
  3.75216766267008*^9}},ExpressionUUID->"9af40ae2-e134-40ab-9602-\
e05f1df3ee30"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"nDailyRiskFree", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "+", "0.02"}], ")"}], "^", 
     RowBox[{"(", 
      RowBox[{"1", "/", "250"}], ")"}]}], "-", "1"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"mnEstBetaErrVar", " ", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"LinearModelFit", "[", 
        RowBox[{
         RowBox[{"Transpose", "[", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"mnReturns", "\[LeftDoubleBracket]", 
              RowBox[{"All", ",", " ", "i"}], "\[RightDoubleBracket]"}], "-", 
             "nDailyRiskFree"}], ",", " ", 
            RowBox[{"vnDJIReturns", "-", "nDailyRiskFree"}]}], "}"}], "]"}], 
         ",", " ", "\[FormalX]", ",", " ", "\[FormalX]", ",", " ", 
         RowBox[{"IncludeConstantBasis", "\[Rule]", "False"}]}], "]"}], "[", 
       RowBox[{"{", 
        RowBox[{
        "\"\<BestFitParameters\>\"", ",", " ", "\"\<EstimatedVariance\>\""}], 
        "}"}], "]"}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", 
       RowBox[{"Length", "[", "vsDJI", "]"}]}], "}"}]}], "]"}]}], 
  ";"}]}], "Input",
 CellChangeTimes->{{3.75333662885294*^9, 3.7533366598716135`*^9}, {
  3.7533367169807453`*^9, 3.753336759781845*^9}, {3.7533369685069504`*^9, 
