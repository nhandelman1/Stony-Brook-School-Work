Cell["\<\
Compute and plot a mean-variance efficient frontier for a portfolio \
consisting of these two assets.\
\>", "Item",
 CellChangeTimes->{{3.746287748303142*^9, 
  3.74628779667108*^9}},ExpressionUUID->"0a77352c-a469-4f8b-970d-\
ac17b21cfed3"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"xOptimalPortfolio", "[", 
    RowBox[{"mnCov_", ",", "vnMean_", ",", "\[Lambda]_"}], "]"}], ":=", 
   RowBox[{"Module", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"mi", ",", "v1", ",", "v\[Mu]"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"mi", "=", 
       RowBox[{"Inverse", "[", "mnCov", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"v1", "=", 
       RowBox[{"Total", "/@", "mi"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"v\[Mu]", "=", 
       RowBox[{"mi", ".", "vnMean"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"\[Lambda]", " ", "v\[Mu]"}], "+", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{"1", "-", 
            RowBox[{"\[Lambda]", " ", 
             RowBox[{"Total", "[", "v\[Mu]", "]"}]}]}], ")"}], "/", 
          RowBox[{"Total", "[", "v1", "]"}]}], ")"}], "v1"}]}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"xPortSdevMean", "[", 
    RowBox[{"x_", ",", 
     RowBox[{"{", 
      RowBox[{"\[Mu]_", ",", "\[CapitalSigma]_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     SqrtBox[
      RowBox[{"x", ".", "\[CapitalSigma]", ".", "x"}]], ",", 
